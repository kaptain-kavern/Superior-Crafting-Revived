<?xml version="1.0" encoding="utf-8" ?>
<Defs>    
<!-- Making stone blocks also drop a bit of sand -->
    <Override.Def Target="MakeStoneBlocks">
        <products>
            <Sand>1</Sand>
        </products>
    </Override.Def>  

    <Override.Def Target="ArtilleryShell">
      <label>SC_ArtilleryShell</label>
      <statBases Mode="Replace">
        <WorkToMake>800</WorkToMake>
      </statBases>
      <costList>
        <SteelBar>5</SteelBar>
      </costList>      
      <recipeMaker>
        <workSpeedStat>SmithingSpeed</workSpeedStat>
        <workSkill>Crafting</workSkill>
        <effectWorking>Smith</effectWorking>
        <soundWorking>Recipe_Smith</soundWorking>        
        <recipeUsers>
          <li>TableMachining</li>
          <li>TableSmithing</li>
        </recipeUsers>        
        <unfinishedThingDef>UnfinishedGun</unfinishedThingDef>
      </recipeMaker>
    </Override.Def>

</Defs>